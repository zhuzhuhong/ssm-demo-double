<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
    <!--配置控制台输出-->
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </appenders>
    <!--指定日志级别-->
    <loggers>
        <!--全局日志级别-->
        <root level="info">
            <appender-ref ref="Console"/>
        </root>
    </loggers>
</configuration>